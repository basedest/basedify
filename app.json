{
    "expo": {
        "name": "basedify",
        "slug": "basedify",
        "version": "1.0.0",
        "scheme": "basedify",
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": "./assets/favicon.png"
        },
        "plugins": [
            "expo-router",
            "@prisma/react-native",
            [
                "expo-dev-launcher",
                {
                    "launchMode": "most-recent"
                }
            ],
            "expo-localization"
        ],
        "experiments": {
            "typedRoutes": true,
            "tsconfigPaths": true
        },
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true,
            "bundleIdentifier": "com.basedest.basedify"
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#ffffff"
            },
            "package": "com.basedest.basedify"
        },
        "extra": {
            "router": {
                "origin": false
            },
            "eas": {
                "projectId": "df2d35c4-59c7-4685-82f7-2bc2f8c6cdac"
            }
        }
    }
}

{
  "name": "basedify",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "ios": "DARK_MODE=media expo run:ios",
    "android": "DARK_MODE=media expo run:android",
    "build:dev": "eas build --profile development",
    "build:preview": "eas build --profile preview",
    "build:prod": "eas build --profile production",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "web": "DARK_MODE=media expo start --web",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/html-elements": "^0.4.2",
    "@expo/vector-icons": "^14.0.0",
    "@gluestack-ui/button": "^1.0.8",
    "@gluestack-ui/form-control": "^0.1.19",
    "@gluestack-ui/nativewind-utils": "^1.0.23",
    "@gluestack-ui/overlay": "^0.1.15",
    "@gluestack-ui/toast": "^1.0.7",
    "@prisma/client": "5.22.0",
    "@prisma/react-native": "5.22.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/native": "^6.1.7",
    "@rn-primitives/slot": "^1.1.0",
    "@rn-primitives/toggle": "^1.1.0",
    "@rn-primitives/toggle-group": "^1.1.0",
    "@rn-primitives/types": "^1.1.0",
    "@tanstack/react-query": "^5.59.20",
    "babel-plugin-module-resolver": "^5.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "expo": "~51.0.39",
    "expo-constants": "~16.0.1",
    "expo-dev-client": "~4.0.14",
    "expo-dev-launcher": "^4.0.22",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.24",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.4",
    "expo-web-browser": "~13.0.3",
    "lucide-react-native": "^0.460.0",
    "nativewind": "^4.0.36",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.53.2",
    "react-native": "0.74.5",
    "react-native-css-interop": "^0.0.36",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-quick-base64": "2.1.2",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "tailwind-merge": "^2.5.4",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.1.2",
    "eslint-config-universe": "^12.0.1",
    "jscodeshift": "^0.15.2",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.3.3"
  },
  "eslintConfig": {
    "extends": "universe/native",
    "root": true
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
  },
  "private": true
